<div class="row">
  <h1 class="mb-3">投稿一覧</h1>
</div>
<div class="row">
  <div class="mb-3">
    <%= form_with url: posts_path, method: :get do |f| %>
      <%= f.search_field :keyword, value: params[:keyword] %>
      <%= f.submit "検索" %>
    <% end %>
    </div>
</div>

<%= render 'index', posts: @posts %>
